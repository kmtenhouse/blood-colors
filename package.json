{
  "name": "blood-colors",
  "version": "1.0.0",
  "description": "Tool to sort blood colors",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "seed": "node database/seeds/index.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmtenhouse/blood-colors.git"
  },
  "author": "Katherine Tenhouse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kmtenhouse/blood-colors/issues"
  },
  "homepage": "https://github.com/kmtenhouse/blood-colors#readme",
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "brain.js": "^2.0.0-alpha.9",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "ejs": "^2.7.3",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "helmet-csp": "^2.9.4",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "mongoose-seed": "^0.6.0",
    "nodemon": "^1.19.2",
    "require-all": "^3.0.0"
  }
}
